{
  "expo": {
    "name": "Goddess Story Library",
    "slug": "goddess_story_library",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#f2a4a8"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.silversoft.goddess-story-library",
      "infoPlist": {
        "NSSpeechRecognitionUsageDescription": "This app uses speech recognition to convert your speech to text.",
        "NSCameraUsageDescription": "This app uses the camera to let user put a photo in his profile page."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#f2a4a8"
      },
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "com.silversoft.goddess_story_library"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["@react-native-voice/voice"],

    "extra": {
      "eas": {
        "projectId": "3ecac027-9d6d-4e86-878c-e8ba23c725d2"
      }
    }
  }
}
